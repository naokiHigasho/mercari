.items.clearfix
  - @items01.each do |item|
    = render partial: 'shared/item-square200', locals: {item: item}
  = link_to_next_page @items01, next_icon, class: "page-arrow left-arrow", remote: true, :params => {:view_num => 1}
  = link_to_prev_page @items01, prev_icon, class: "page-arrow right-arrow", remote: true, :params => {:view_num => 1}
